{
  "expo": {
    "name": "My Todo App List",
    "slug": "my-todo-app-list",
    "version": "1.1.6",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "my-todo-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "privacy": "public",
    "description": "Application de gestion de tâches avec système d'attribution administrateur",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "backgroundColor": "#2563EB",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.superflyman90.mytodoapplist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.superflyman90.mytodoapplist",
      "versionCode": 5
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "700d1845-2425-4ee4-8f0b-f24640b5f8ac"
      }
    },
    "owner": "superflyman90"
  }
}
